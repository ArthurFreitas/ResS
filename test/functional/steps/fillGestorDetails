package steps

import gestorCadastre.gestorCadastreController

class fillGestorDetails{

    static  generator = [

            [name:"joao",
             CNPJGenerator: "Restaurante",
             address: "Bubble Street number 7",
             telephone: 91-44321124,
             email: "anyemail@hotmail.com",
             website: "http://2.bp.blogspot.com/-ZVxEkrY04Fs/U15D5uCL-xI/AAAAAAAAOdQ/pJ-Wc99LmPY/s1600/logo_col_874x288.png"]
    ]

    static public def findByCNPJ(String cnpj) {
        gestor.find { gestor ->
            generator.CNPJGenerator == cnpj
        }
    }
         
